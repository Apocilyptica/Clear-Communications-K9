{
  "name": "fusionary-gatsby-ts-tw-template",
  "version": "1.0.0",
  "private": true,
  "description": "Template for use with Fusionary's figma standard for Tailwind.",
  "author": "James Jager",
  "keywords": [
    "gatsby",
    "Tailwind",
    "Typescript"
  ],
  "scripts": {
    "develop": "ENABLE_GATSBY_REFRESH_ENDPOINT=true gatsby develop",
    "start": "yarn clean && yarn develop & yarn tw:watch",
    "build": "GATSBY_EXPERIMENTAL_PAGE_BUILD_ON_DATA_CHANGES=true yarn tw:build && gatsby build",
    "tw": "npx tailwindcss  -i ./src/tailwind.css -o ./src/styles/tailwind-generated.css",
    "tw:build": "yarn tw --minify",
    "tw:watch": "yarn tw --watch",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "typecheck": "tsc --noEmit",
    "lint": "eslint --ignore-path .gitignore .",
    "lint:fix": "npm run lint -- --fix"
  },
  "dependencies": {
    "@babel/core": "^7.18.2",
    "@babel/plugin-transform-runtime": "^7.17.12",
    "@babel/preset-env": "^7.18.2",
    "@headlessui/react": "^1.6.1",
    "@heroicons/react": "^1.0.6",
    "@loadable/component": "^5.15.2",
    "@tailwindcss/aspect-ratio": "^0.4.0",
    "@tailwindcss/forms": "^0.5.2",
    "@tailwindcss/line-clamp": "^0.4.0",
    "@tailwindcss/typography": "^0.5.2",
    "@testing-library/jest-dom": "^5.16.4",
    "autoprefixer": "^10.4.7",
    "axios": "^0.27.2",
    "classnames": "^2.3.1",
    "gatsby": "^4.17.0",
    "gatsby-background-image": "^1.6.0",
    "gatsby-plugin-google-analytics": "^4.17.0",
    "gatsby-plugin-image": "^2.17.0",
    "gatsby-plugin-loadable-components-ssr": "^4.2.0",
    "gatsby-plugin-manifest": "^4.17.0",
    "gatsby-plugin-react-helmet": "^5.17.0",
    "gatsby-plugin-react-svg": "^3.1.0",
    "gatsby-plugin-root-import": "^2.0.8",
    "gatsby-plugin-sharp": "^4.17.0",
    "gatsby-plugin-sitemap": "^5.14.0",
    "gatsby-source-filesystem": "^4.17.0",
    "gatsby-transformer-sharp": "^4.17.0",
    "gbimage-bridge": "^0.2.1",
    "lodash": "^4.17.21",
    "prettier": "^2.6.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "slugify": "^1.6.5",
    "tailwindcss": "^3.1.4",
    "usehooks-ts": "^2.6.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.18.2",
    "@testing-library/jest-dom": "^5.16.4",
    "@types/axios": "^0.14.0",
    "@types/jest": "^27.5.1",
    "@types/loadable__component": "^5.13.4",
    "@types/lodash": "^4.14.182",
    "@types/node": "^18.0.0",
    "@types/reach__router": "^1.3.10",
    "@types/react": "^18.0.14",
    "@types/react-dom": "^18.0.5",
    "@types/react-helmet": "^6.1.5",
    "@types/react-select": "^5.0.1",
    "@types/tailwindcss": "^3.0.10",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^8.18.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "graphql-tag": "^2.12.6",
    "graphql-typegen": "^1.2.2",
    "jscodeshift": "^0.13.1",
    "npm": "^8.13.2",
    "ts-loader": "^9.3.0",
    "typescript": "^4.7.4"
  },
  "resolutions": {
    "ansi-regex": "^5.0.1",
    "browserslist": "^4.16.5",
    "graphql": "^15.4.0",
    "postcss": "^8.2.13",
    "webpack": "^5.24.2"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json}": [
      "eslint --fix"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
